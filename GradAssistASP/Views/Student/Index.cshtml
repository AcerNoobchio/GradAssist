@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <table>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.L_Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.M_Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.F_Name)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.L_Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.M_Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.F_Name)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                    </td>
                </tr>
            }
</div>
</body>
</html>
